{
  "name": "globalartpro-backend",
  "version": "1.0.0",
  "description": "Backend API for GlobalArtPro project",
  "type": "module",
  "main": "server.js",
  "scripts": {
  "dev": "nodemon src/index.js",
  "migrate": "knex migrate:latest",
  "seed": "knex seed:run",
  "start": "node src/index.js",
  "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --runInBand --detectOpenHandles"
},
  "keywords": [
    "express",
    "api",
    "globalartpro",
    "backend"
  ],
  "author": "TeKeu",
  "license": "ISC",
  "dependencies": {
    "axios": "^1.12.2",
    "bcryptjs": "^3.0.3",
    "multer": "^1.4.5-lts.1",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "mongodb": "^6.20.0",
    "mongoose": "^8.19.2",
    "openai": "^6.2.0",
    "pg": "^8.16.3",
    "replicate": "^1.3.0"
  },
  "devDependencies": {
    "nodemon": "^3.1.11",
    "jest": "^29.0.0",
    "supertest": "^6.3.0",
    "cross-env": "^7.0.3"
  },
  "optionalDependencies": {
    "minio": "latest"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "jest": {
    "testEnvironment": "node",
    "testMatch": ["**/test/**/*.test.js"]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/noukawo81/globalartpro-backend.git"
  },
  "bugs": {
    "url": "https://github.com/noukawo81/globalartpro-backend/issues"
  },
  "homepage": "https://github.com/noukawo81/globalartpro-backend#readme"
}
